    "from functools import reduce\n",
    "i=int(input())\n",
    "arr=[]\n",
    "arr[0:i]=map(int,input().split())\n",
    "arr.sort()\n",
    "k=0\n",
    "for x in arr:\n",
    "    arr[k]=x/arr[i-1]*100\n",
    "    k+=1\n",
    "o=reduce(lambda x,y:x+y,arr)\n",
    "print(o/i)"
